package calculatorApp;

public class Calculator {

	public void sum(int a, int b) {
		System.out.println(a+b);
	}
	
	public void subtraction(int a, int b) {
		System.out.println(a-b);
	}
	
	public void multiplication(int a, int b) {
		System.out.println(a*b);
	}
	
	public void division(int a, int b) {
		System.out.println(a/b);
	}
}
